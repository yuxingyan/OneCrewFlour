package cn.com.pojo;

import java.util.Date;

import org.springframework.stereotype.Component;

/**
 * Payout entity. @author MyEclipse Persistence Tools
 */
@Component("payout")
public class Payout implements java.io.Serializable {

	// Fields

	private Integer poid;
	private String write;
	private String type;
	private Float sum;
	private Date date;

	// Constructors

	/** default constructor */
	public Payout() {
	}

	/** full constructor */
	public Payout(String write, String type, Float sum, Date date) {
		this.write = write;
		this.type = type;
		this.sum = sum;
		this.date = date;
	}

	// Property accessors

	public Integer getPoid() {
		return this.poid;
	}

	public void setPoid(Integer poid) {
		this.poid = poid;
	}

	public String getWrite() {
		return this.write;
	}

	public void setWrite(String write) {
		this.write = write;
	}

	public String getType() {
		return this.type;
	}

	public void setType(String type) {
		this.type = type;
	}

	public Float getSum() {
		return this.sum;
	}

	public void setSum(Float sum) {
		this.sum = sum;
	}

	public Date getDate() {
		return this.date;
	}

	public void setDate(Date date) {
		this.date = date;
	}

}